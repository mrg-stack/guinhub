      color: #1437a2;
      font-weight: 700;
      border-bottom: 2px solid #1437a222;
      padding-bottom: 4px;
      font-size: 1.2em;
    }

    .link-grid {
      display: grid;
      grid-template-columns: repeat(auto-fill, minmax(200px, 1fr));
      gap: 20px;
      margin-top: 20px;
    }

    .link-card {
      background: white;
      border-radius: 12px;
      padding: 20px;
      text-align: center;
      text-decoration: none;
      color: #1437a2;
      font-weight: 600;
      box-shadow: 0 3px 8px rgba(0,0,0,0.05);
      transition: all 0.2s ease-in-out;
      display: flex;
      flex-direction: column;
      align-items: center;
      gap: 8px;
      font-size: 1em;
    }

    .link-card i {
      font-size: 1.8em;
    }

    .link-card:hover {
      background: #1437a2;
      color: white;
      transform: translateY(-3px);
    }

    .link-card:hover i {
      color: white;
    }

    .search-form {
      margin: 30px 0 50px;
      display: flex;
      justify-content: center;
      gap: 0.5rem;
    }

    .search-form input {
      padding: 0.7rem 1rem;
      border-radius: 8px;
      border: 1px solid #ccc;
      width: 280px;
      font-size: 1em;
    }

    .search-form button {
      padding: 0.7rem 1.2rem;
      background-color: #1437a2;
      color: white;
      border: none;
      border-radius: 8px;
      cursor: pointer;
      font-weight: 600;
      font-size: 1em;
      display: flex;
      align-items: center;
      gap: 6px;
      transition: background 0.2s;
    }

    .search-form button:hover {
      background-color: #002a80;
    }

    footer {
      position: relative;
      text-align: center;
      padding: 20px 10px;
      font-size: 0.9em;
      color: #1437a2;
      margin-top: 50px;
      background-color: #59a8b3;
      border-top: 2px solid #1437a222;
      min-height: 40px;
    }

    .corner-logo {
      position: absolute;
      bottom: 10px;
      right: 20px;
      width: 250px;
      opacity: 1;
      transition: opacity 0.3s;
    }

    .corner-logo:hover {
      opacity: 1;
    }

    /* Collapsible sections */
    .collapsible-content {
      max-height: 0;
      overflow: hidden;
      transition: max-height 0.35s ease;
    }

    .collapsible-header {
      cursor: pointer;
    }

    /* E25 A/B two aligned columns */
    .e25-columns {
      display: grid;
      grid-template-columns: repeat(2, minmax(0, 1fr));
      gap: 40px;
      margin-top: 20px;
      align-items: flex-start;
      position: relative;
    }

    /* vertical separator line */
    .e25-columns::before {
      content: "";
      position: absolute;
      top: 0;
      bottom: 0;
      left: 50%;
      width: 2px;
      background: #1437a222;
    }

    .e25-column h3 {
      margin-top: 0;
      border-bottom: none !important;
      padding-bottom: 0 !important;
    }

    @media (max-width: 700px) {
      .e25-columns {
        grid-template-columns: 1fr;
      }
      .e25-columns::before {
        display: none;
      }
    }
